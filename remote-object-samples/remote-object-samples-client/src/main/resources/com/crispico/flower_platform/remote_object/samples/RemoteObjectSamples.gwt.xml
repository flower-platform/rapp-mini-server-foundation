<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.1//EN"
        "https://raw.githubusercontent.com/gwtproject/gwt/2.8.1/distro-source/core/src/gwt-module.dtd">
        
<module rename-to="RemoteObjectSamples">
	<inherits name="com.crispico.foundation.client.FoundationClientAppBase" />
	
	<!-- 
		This module stays in the root so that we can access resources directly from the root
		of the classpath (e.g. from node_modules, which is configured as a resources dir).
	 -->
	<inherits name="RemoteObjectSamplesResources"/>

	<source path='client' />
	<source path='shared' />

	<extend-configuration-property name="gin.ginjector.modules" value="com.crispico.flower_platform.remote_object.samples.client.AggregatedGinModule" />
	<extend-configuration-property name="gin.ginjector.modules" value="com.crispico.flower_platform.remote_object.samples.client.MainGinModule" />

	<set-configuration-property name="gwtp.bootstrapper" value="com.crispico.flower_platform.remote_object.samples.client.ClientBootstrapper" />
</module>